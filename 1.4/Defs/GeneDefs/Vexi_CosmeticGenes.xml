<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <GeneDef ParentName="GeneHeadboneBase">
        <defName>dIl_Vexi_FringeA</defName>
        <label>vexine facial fur</label>
        <description>This gene codes for a certain volume of fur on the vexi head.</description>
        <iconPath>ui/Xenotypes/VexiSp</iconPath>
        <graphicData>
            <graphicPath>Things/Pawn/Humanlike/HeadAttachments/FringeA</graphicPath>
            <!-- <narrowCrownHorizontalOffset>0.03</narrowCrownHorizontalOffset> -->
            <useSkinShader>false</useSkinShader>
            <drawScale>1</drawScale>
            <drawOffsetEast>0,0.55,0</drawOffsetEast>
            <drawOffsetSouth>0,0.55,0</drawOffsetSouth>
            <!--<drawOffsetEast>(0.26, 0, -0.125)</drawOffsetEast>
    <drawOffsetSouth>(-0.01, 0, -0.13)</drawOffsetSouth>-->
            <layer>PostSkin</layer>
            <colorType>Skin</colorType>
        </graphicData>
        <missingGeneRomanceChanceFactor>0.2</missingGeneRomanceChanceFactor>
        <displayCategory>dIl_Vexi_Endogenes</displayCategory>
        <displayOrderInCategory>2</displayOrderInCategory>
        <symbolPack>
            <prefixSymbols>
                <li>
                    <symbol>vexine</symbol>
                </li>
                <li>
                    <symbol>fringer</symbol>
                </li>
            </prefixSymbols>
            <suffixSymbols>
                <li>
                    <symbol>vulpine</symbol>
                </li>
                <li>
                    <symbol>face</symbol>
                </li>
            </suffixSymbols>
        </symbolPack>
    </GeneDef>
